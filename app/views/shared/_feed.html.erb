<% if @feed_items.try(:any?) %>
  <% @feed_items.each do |image| %>
    <% if image.available?(current_user) %>
      <%= link_to image.user.name, user_path(id: image.user.id) %>
      <%= link_to image_path image do %>
        <%= image_tag image.imager.feed  %>
      <% end %>
      <%= time_ago_in_words(image.created_at) %> ago
    <% end %>
  <% end %>
<% end %>